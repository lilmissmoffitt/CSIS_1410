package wrapperClass;

/**
 * 
 * @author melissamoffitt
 *
 */
public class DemoWrapperClass {
	
	public static void main(String[] args) {
		minMax();
	}
	
	/**
	 * 
	 * @return 
	 * @return
	 */
	public static void minMax() {
		System.out.printf("Byte    : [%d , %d] ", Byte.MIN_VALUE, Byte.MAX_VALUE);
		System.out.printf("Short   : [%d , %d] ", Short.MIN_VALUE, Short.MAX_VALUE);
		System.out.printf("Integer : [%d , %d] ", Integer.MIN_VALUE, Integer.MAX_VALUE);
		System.out.printf("Long    : [%d , %d] ", Long.MIN_VALUE, Long.MAX_VALUE);
		System.out.printf("Float   : [%d , %d] ", Float.MIN_VALUE, Float.MAX_VALUE);
		System.out.printf("Double  : [%d , %d] ", Double.MIN_VALUE, Double.MAX_VALUE);
	}
	
	

}
