package nuisance;

public class Friend extends Person{
	private Hobby hobby;
	
	public Friend(String name, int age, Hobby hobby) {
		super(name, age);
		this.hobby = hobby;
	}
	
	public String chill() {
		return getName() + " is chilling";
	}
	
	public String play(Friend[] friends) {
		return "J";
	}
	
	
}
